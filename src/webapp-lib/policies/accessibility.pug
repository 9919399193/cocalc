//- accessibility page, includes the statically react.js rendered _accessibility.html

extends ../_base.pug

block vars
  -
    var navbar_active = 'accessibility';
    var subtitle = 'Accessibility';
    var subnavbar_active = 'accessibility';

block content

  div.jumbotron
    div.container
      h1 #[a(href=URL) #{COMPANY_NAME}]
      h2 #{NAME} Accessibility (July 2019)
      a.btn.btn-default(href="index.html" role="button") &laquo; Other Policies

  div.container.vertical-expand
    include _accessibility.html
